<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <customBinding>
                <binding name="extranet">
                    <binaryMessageEncoding />
                    <httpsTransport />
                </binding>
            </customBinding>
            <netTcpBinding>
                <binding name="intranet" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8081/tigli/intranet" binding="netTcpBinding"
                bindingConfiguration="intranet" contract="ServiceTigli.ITigliService"
                name="intranet">
                <identity>
                    <userPrincipalName value="zxxx\user" />
                </identity>
            </endpoint>
            <endpoint address="https://localhost:8082/tigli/extranet" binding="customBinding"
                bindingConfiguration="extranet" contract="ServiceTigli.ITigliService"
                name="extranet" />
        </client>
    </system.serviceModel>
</configuration>